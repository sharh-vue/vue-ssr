{
  "name": "ssr",
  "version": "1.0.0",
  "description": "server-side-ssr",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch server/server.js --watch webpack/utils/autoGenerateRoutes.js --watch webpack/client.config.js --watch webpack/server.config.js --watch webpack/base.config.js --inspect server/server.js",
    "server": "cross-env process.env.BUILD_ENV=production process.env.NODE_ENV=production webpack --display-error-details --config webpack/server.config.js",
    "client": "cross-env process.env.BUILD_ENV=production process.env.NODE_ENV=production webpack --profile --display-error-details --config webpack/client.config.js",
    "router": "cross-env process.env.BUILD_ENV=production process.env.NODE_ENV=production node createRouter.js",
    "build": "npm run router && npm run server && npm run client",
    "debug": "node --inspect node_modules/webpack/bin/webpack.js --config webpack/client.config.js",
    "start": "node server/server"
  },
  "author": "sharh<609557623@qq.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "morgan": "^1.9.0",
    "postcss": "^7.0.2",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-px2rem": "^0.3.0",
    "precss": "^3.1.2",
    "url-loader": "^1.0.1",
    "vue": "^2.5.17",
    "vue-loader": "^15.2.6",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^2.6.1",
    "workbox-webpack-plugin": "^3.4.1"
  },
  "devDependencies": {
    "chokidar": "^2.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-hot-loader": "^1.4.1",
    "memory-fs": "^0.4.1",
    "nodemon": "^1.18.3",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpackbar": "^2.6.1",
    "prettier": "^1.14.0"
  }
}
